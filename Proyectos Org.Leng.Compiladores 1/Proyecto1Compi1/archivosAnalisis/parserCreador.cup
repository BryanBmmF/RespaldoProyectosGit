	public Alineaciones retornarAlineacionTipo(String tipo){
		switch(tipo){
			case "CENTRAR":
				return this.alineacionTipo.CENTRAR;
			case "DERECHA":
				return this.alineacionTipo.DERECHA;
			case "IZQUIERDA":
				return this.alineacionTipo.IZQUIERDA;
			case "JUSTIFICAR":
				return this.alineacionTipo.JUSTIFICAR;

		}
		return null;
	}

