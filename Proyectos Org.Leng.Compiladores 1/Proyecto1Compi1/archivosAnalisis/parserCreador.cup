	/*metodos para manejo de etiquetas*/
	public void agregarEtiqueta(String valor){
			boolean add=true;
			for(Etiqueta eti: this.listaEtiquetas){
				if (eti.getValor().equals(valor)){
					/* code */
					add=false;
					break;
				}
			}
			if (add){
				listaEtiquetas.add(new Etiqueta(valor));
			} else {
				listaErrores.add("Error: Existen etiquetas con el mismo nombre en la accion NUEVA_PAGINA...");
			}
	}


	public void setAtributoEtiqueas(String etiquetas){
			String[] lista = etiquetas.split("\\|");
			for(String s:lista){
            		agregarEtiqueta(s);
        	}
	}


