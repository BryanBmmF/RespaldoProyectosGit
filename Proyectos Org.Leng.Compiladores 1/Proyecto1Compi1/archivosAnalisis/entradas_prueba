<accion nombre="MODIFICAR_PAGINA">
    <parametros>
           <parametro nombre="ID">
                  [_productos11]
            </parAmetRo>
           <parametro nombre="TITULO">
                  [nuevo titulo p1h1]
            </parAmetRo>
              
    </parametros>
  <etiquetas>
            <etiqueta valor= "etiqueta1"/ >
            <etiqueta valor= "etiqueta2"/ >
    </etiquetas>
</accion>

<accion nombre= "MODIFICAR_COMPONENTE" >
    <parametros>
            <parametro nombre= "ID" >
                  [_Titulo-2]
            </parametro>
            <parametro nombre= "PAGINA" >
                  [_historia2]
            </parametro>
            <parametro nombre= "CLASE" >
                  [TITULO]
            </parametro>
    </parametros>
    <atributos>
            <atributo nombre= "TEXTO" >
                  [TITULO MODIFICADO :) ]
            </atributo>
            <atributo nombre= "ALINEACION" >
                  [CENTRAR]
            </atributo>
            <atributo nombre= "COLOR" >
                  [#0000FF]
            </atributo>
    </atributos>
</accion>
