<accion nombre= "AGREGAR_COMPONENTE" >
    <parametros>
            <parametro nombre= "ID" >
                  [_Titulo-1]
            </parametro>
            <parametro nombre= "PAGINA" >
                  [_pagina1]
            </parametro>
            <parametro nombre= "CLASE" >
                  [TITULO]
            </parametro>
    </parametros>
    <atributos>
            <atributo nombre= "TEXTO" >
                  [Este es el texto que aparece en el titulo1 :) ]
            </atributo>
            <atributo nombre= "ALINEACION" >
                  [JUSTIFICAR]
            </atributo>
            <atributo nombre= "COLOR" >
                  [#5A5A5A]
            </atributo>
    </atributos>
</accion>

