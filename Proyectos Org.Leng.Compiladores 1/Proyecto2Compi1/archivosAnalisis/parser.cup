	public Boolean evaluarCondicionRelacional(Integer v1, Integer v2, String op){
			switch (op){
				case "<" : {
					if (v1.intValue()<v2.intValue()){
							return new Boolean(Boolean.TRUE);
					} else {
							return new Boolean(Boolean.FALSE);
					}	
					
				}
				case ">" : {
					if (v1.intValue()>v2.intValue()){
							return new Boolean(Boolean.TRUE);
					} else {
							return new Boolean(Boolean.FALSE);
					}	

					
				}
				case "<=" : {
					if (v1.intValue()<=v2.intValue()){
							return new Boolean(Boolean.TRUE);
					} else {
							return new Boolean(Boolean.FALSE);
					}	
					
				}
				case ">=" : {
					if (v1.intValue()>=v2.intValue()){
							return new Boolean(Boolean.TRUE);
					} else {
							return new Boolean(Boolean.FALSE);
					}	
					
				}
				case "<>" : {
					if (v1.intValue()!=v2.intValue()){
							return new Boolean(Boolean.TRUE);
					} else {
							return new Boolean(Boolean.FALSE);
					}	
					
				}

			}
			return null;

	}
	
